{
	"name": "rpi-web-robot",
	"version": "1.0.0",
	"main": "index.js",
	"repository": "https://github.com/king-numsgil/rpi-web-robot.git",
	"author": "Daniel Grondin <king@numsgil.co>",
	"license": "MIT",
	"private": true,
	"scripts": {
		"web:dev": "vite --host",
		"web:build": "tsc && vite build",
		"web:serve": "vite preview",
		"api:build": "tsc --project ./backend/tsconfig.json",
		"api:build:w": "tsc -w --project ./backend/tsconfig.json",
		"api:dev": "nodemon --delay 500ms --watch \"dist/**/*.js\" --ignore \"node_modules/**/*\" --exec node ./dist/index.js",
		"api:start": "node ./dist/index.js",
		"build": "yarn web:build && yarn api:build"
	},
	"devDependencies": {
		"@types/body-parser": "^1.19.1",
