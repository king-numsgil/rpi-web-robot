		"@tsed/exceptions": "^6.73.3",
		"@tsed/json-mapper": "^6.73.3",
		"@tsed/logger": "^5.17.0",
		"@tsed/openspec": "^6.73.3",
		"@tsed/platform-express": "^6.73.3",
		"@tsed/schema": "^6.73.3",
		"@tsed/socketio": "^6.73.3",
		"body-parser": "^1.19.0",
