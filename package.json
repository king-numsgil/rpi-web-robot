		"@types/multer": "^1.4.7",
		"@types/node": "^16.10.2",
		"@types/react": "^17.0.27",
		"@types/react-dom": "^17.0.9",
		"@types/socket.io": "^3.0.2",
		"@types/socket.io-client": "^3.0.0",
		"@vitejs/plugin-react": "^1.0.1",
		"@vitejs/plugin-react-refresh": "^1.3.6",
		"nodemon": "^2.0.13",
		"typescript": "^4.4.3",
		"vite": "^2.6.2",
		"vite-react-jsx": "^1.1.2",
		"vite-tsconfig-paths": "^3.3.14"
	},
	"dependencies": {
